= form_with model: administrator, url: url, method: method, local: true do |f|
  - if @administrator.errors.any?
    #error_explanation
      %h2= "#{pluralize(@administrator.errors.count, "error")} prohibited this administrator from being saved:"
      %ul
        - @administrator.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :email
    = f.text_field :email
  .field
    = f.label :password
    = f.password_field :password
  .field
    = f.label :password_confirmation
    = f.password_field :password_confirmation
  .actions
    = f.submit 'Save'
